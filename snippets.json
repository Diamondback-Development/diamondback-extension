{
    "Variable Assignment": {
      "prefix": "var",
      "body": [
        "var $1 = $2"
      ],
      "description": "Assign a value to a variable"
    },
    "Print Statement": {
      "prefix": "print",
      "body": [
        "print($1)"
      ],
      "description": "Print a value to the console"
    },
    "Function Definition": {
      "prefix": "def",
      "body": [
        "def $1()",
        "  $2",
        "endf"
      ],
      "description": "Define a new function"
    },
